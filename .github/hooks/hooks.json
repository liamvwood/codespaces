{
  "version": 1,
  "hooks": {
    "sessionStart": [
      {
        "type": "command",
        "bash": ".github/scripts/session_start.sh",
        "cwd": ".",
        "timeoutSec": 10
      }
    ],
    "sessionEnd": [
      {
        "type": "command",
        "bash": ".github/scripts/session_end.sh",
        "cwd": ".",
        "timeoutSec": 10
      }
    ]
  }
}
